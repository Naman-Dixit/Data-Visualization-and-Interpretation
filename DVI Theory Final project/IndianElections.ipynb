# 1) Imports
import os
from google.colab import files
import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
import seaborn as sns

uploaded = files.upload()  # choose the excel file

# 3) Read excel (first sheet) - robust to CSV or Excel
fname = list(uploaded.keys())[0]
if fname.lower().endswith(('.xls', '.xlsx')):
    df = pd.read_excel(fname, engine='openpyxl')
else:
    # assume CSV
    df = pd.read_csv(fname)

print("\n=== Data preview ===")
display(df.head())
print("\nShape:", df.shape)
print("\nColumns:\n", df.columns.tolist())

def find_column_by_keywords(df, keywords):
    for keyword in keywords:
        for col_name in df.columns:
            if keyword.lower() in col_name.lower():
                return col_name
    return None

# try to auto-detect useful columns
const_col = find_column_by_keywords(df, ['constituency', 'const', 'seat', 'riding', 'ward'])
candidate_col = find_column_by_keywords(df, ['candidate', 'cand', 'name', 'nominee'])
party_col = find_column_by_keywords(df, ['party', 'partyname', 'party_name', 'partyabbr'])
votes_col = find_column_by_keywords(df, ['votes', 'vote', 'votes_obtained', 'total_votes', 'votes_received', 'valid_votes'])
total_votes_col = find_column_by_keywords(df, ['total votes', 'totalvotes', 'total_votes', 'electors', 'turnout'])

print("\nAuto-detected columns:")
print("Constituency column:", const_col)
print("Candidate column:", candidate_col)
print("Party column:", party_col)
print("Votes column:", votes_col)
print("Total votes column:", total_votes_col)

# 5) If no explicit votes column, try to find numeric column that likely holds votes
if votes_col is None:
    numeric_cols = df.select_dtypes(include=[np.number]).columns.tolist()
    print("Numeric columns:", numeric_cols)
    if len(numeric_cols) == 1:
        votes_col = numeric_cols[0]
        print("Assuming votes column is:", votes_col)
    else:
        # leave None; we'll try to aggregate on numeric columns later
        print("No clear votes column auto-detected. You may need to rename or set votes column manually.")


# 6) Create a Win/Loss target robustly
# Approach:
# - If a column likely named 'winner'/'is_winner'/'result' exists, use it.
# - Else, if constituency and votes columns available, mark row with max votes per constituency as winner.
# - If none of that works, try to create a proxy target (e.g., top X% of vote_share).
target_col = find_column_by_keywords(df, ['winner', 'is_winner', 'result', 'outcome', 'win'])
print("Detected target column:", target_col)

# Explicitly set const_col and votes_col based on inspection
# This overrides the None values from previous auto-detection if it failed
const_col = 'ac_name'
votes_col = 'totvotpoll'

if target_col is None:
    if const_col and votes_col:
        # ensure votes numeric
        df[votes_col] = pd.to_numeric(df[votes_col], errors='coerce')
        # drop rows without constituency or votes for target creation
        subset = df[[const_col, votes_col]].dropna()
        # compute winner by max votes per constituency
        idx = subset.groupby(const_col)[votes_col].idxmax()
        df['is_winner'] = 0
        df.loc[idx, 'is_winner'] = 1
        target_col = 'is_winner'
        print(f"Created binary target '{target_col}' by picking max {votes_col} per {const_col}.")
    else:
        # fallback: try to create target using vote_share column if present, else create NaNs and stop
        vote_share_col = find_column_by_keywords(df, ['vote_share', 'share', 'vote%','percent'])
        if vote_share_col:
            df['is_winner'] = (df[vote_share_col] >= df[vote_share_col].median()).astype(int)
            target_col = 'is_winner'
            print(f"Created binary target '{target_col}' from median of {vote_share_col}.")
        else:
            raise SystemExit("Could not determine or create a target column automatically. Please ensure your dataset has vote counts and constituency identifiers, or add a 'winner' column.")

# 7) Keep simple features: vote_share and party (if present)
features = []
if 'vote_share' in df.columns:
    features.append('vote_share')
if party_col:
    features.append(party_col)
else:
    print("No party column detected; proceeding with vote_share only.")

# Drop rows without target or feature
df_model = df[features + ['is_winner']].dropna().copy()
print("Modeling rows:", df_model.shape[0])

from sklearn.preprocessing import LabelEncoder

# 8) Encode categorical (party) with LabelEncoder (simple)
le = None
if party_col and party_col in df_model.columns:
    le = LabelEncoder()
    df_model[party_col] = le.fit_transform(df_model[party_col].astype(str))

X = df_model[features]
y = df_model['is_winner'].astype(int)

from sklearn.model_selection import train_test_split
from sklearn.ensemble import RandomForestClassifier

# 9) Train/test split and model
X_train, X_test, y_train, y_test = train_test_split(X, y, test_size=0.2, stratify=y, random_state=42)
model = RandomForestClassifier(n_estimators=100, random_state=42)
model.fit(X_train, y_train)

from sklearn.metrics import accuracy_score, classification_report, confusion_matrix

# 10) Evaluate
y_pred = model.predict(X_test)
print("Accuracy:", round(accuracy_score(y_test, y_pred), 4))
print("\nClassification report:\n", classification_report(y_test, y_pred, zero_division=0))
print("Confusion matrix:\n", confusion_matrix(y_test, y_pred))

# 11) Predict on full dataset and export cleaned CSV
# Prepare a copy with features (apply same encoder if used)
df_export = df.copy()

# If 'vote_share' was one of the features, ensure it's handled.
# The original notebook didn't have 'vote_share' in features for this dataset.
# If vote_share needs NaN handling, do it here.
# pass

if party_col and le:
    # Apply the LabelEncoder transformation to the 'partyname' column directly
    # in df_export. This ensures the column name remains 'partyname' and contains
    # the numerically encoded values, matching the training data's feature name and type.
    df_export[party_col] = le.transform(df_export[party_col].astype(str))

# Now, `df_export` contains the features in the correct format and with the correct names
# as expected by the trained model (e.g., 'partyname' with encoded integers).
# We can simply select these `features` from `df_export` for prediction.
# The `features` variable currently holds `['partyname']`.
preds = model.predict(df_export[features])

df_export['pred_is_winner'] = preds
out_csv = "cleaned_predictions.csv"
df_export.to_csv(out_csv, index=False)
print("Saved:", out_csv)


